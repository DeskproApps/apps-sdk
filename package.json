  "scripts": {
    "start": "pnpm start:storybook",
    "build": "pnpm build:esm && pnpm build:cjs && pnpm build:dist",
    "build:dist": "rm -rf ./dist && rollup --config rollup.config.js && terser dist/deskpro.js -o dist/deskpro.min.js -m --source-map",
    "build:esm": "rm -rf ./esm && tsc",
    "build:cjs": "rm -rf ./cjs && babel ./src --out-dir ./cjs --extensions .ts,.tsx --ignore \"**/*.stories.tsx\" --plugins @babel/plugin-transform-modules-commonjs",
    "watch:dist": "rm -rf ./dist && rollup --config rollup.config.js --watch",
    "build:storybook": "storybook build --quiet -o storybook build",
    "start:storybook": "storybook dev -p 6008",
    "test": "jest",
    "lint": "pnpm eslint --ext ts,tsx ./src",
    "docs:storybook": "storybook build --quiet -o docs/storybook",
    "docs": "pnpm run docs:storybook"
